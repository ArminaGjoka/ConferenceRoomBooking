@model IEnumerable<Final_Project_Conference_Room_Booking.Models.ConferenceRoom>

@{
    ViewData["Title"] = "Index";
}


<div>
    <h4>ConferenceRoom</h4>
    <hr />
    
    <table class="table table-bordered table-hover" > 
        <thead> 
            <tr>
            <th scope="col"> @Html.DisplayNameFor(model => model.Code)  </th>

            <th scope="col"> @Html.DisplayNameFor(model => model.MaxCapacity) </th>
             </tr>    
        </thead>
        <tbody>   

            @foreach(var item in Model)
        {
            <tr>
            <td width="20%"> 
                @Html.DisplayFor(modelitem => item.Code)
            </td>
            <td width="20%"> 
                @Html.DisplayFor(modelitem => item.MaxCapacity)
            </td>

            </tr>
        }

         </tbody>
         </table>   
</div>


